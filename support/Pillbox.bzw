    position 0 0 0
    size 5 5 10
    rotation 0
  end
  pyramid
    position 0 0 10
    size 5 5 0.125
    rotation 0
  end
enddef

define stockade_pair
  group stockade_box
    shift -12.5 0 0
  end
  group stockade_box
    shift 12.5 0 0
  end
enddef

define stockade_quad
  group stockade_pair
    shift -25 0 0
  end
  group stockade_pair
    shift 25 0 0
  end
enddef

define stockade
  group stockade_box
    shift 0 0 0
  end
  group stockade_pair
    shift 37.5 0 0
  end
  group stockade_pair
    spin 90 0 0 1
    shift 0 37.5 0
  end
  group stockade_quad
    shift 25 -23.75 0
  end
  group stockade_quad
    spin 90 0 0 1
    shift -23.75 25 0
  end
enddef

