define pixel
  mesh
  # faces = 1
  # checks = 0
  # vertices = 4
  # normals = 0
  # texcoords = 0
  # mins = 0 -0.5 -0.5
  # maxs = 0 0.5 0.5
    passable
    vertex 0.00000000 -0.50000000 -0.50000000
    vertex 0.00000000 -0.50000000 0.50000000
    vertex 0.00000000 0.50000000 0.50000000
    vertex 0.00000000 0.50000000 -0.50000000
    face
      vertices 0 1 2 3
      matref 0
    endface
  end
enddef

define x_arm
  mesh
  # faces = 1
  # checks = 0
  # vertices = 6
  # normals = 0
  # texcoords = 0
  # mins = 0 -1 -1
  # maxs = 0 1 1
    passable
    vertex 0.00000000 -1.00000000 0.28125000
    vertex 0.00000000 -1.00000000 1.00000000
    vertex 0.00000000 -0.28125000 1.00000000
    vertex 0.00000000 1.00000000 -0.28125000
    vertex 0.00000000 1.00000000 -1.00000000
    vertex 0.00000000 0.28125000 -1.00000000
    face
      vertices 0 1 2 3 4 5
      matref 0
    endface
  end
enddef

